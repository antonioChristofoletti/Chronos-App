<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:layout_editor_absoluteY="25dp">

    <TextView
        android:id="@+id/txtDescricaoBemMaterial_frag_act_principal_os_list_view"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:maxLines="1"
        android:text="Descrição Bem Material"
        android:textColor="@color/colorBlack"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtTipoOcorrenciaBemMaterial_frag_act_principal_os_list_view"
        android:layout_width="0dp"
        android:layout_height="18dp"
        android:text="Tipo Ocorrência"
        android:textSize="12dp"
        app:layout_constraintEnd_toEndOf="@+id/txtDescricaoBemMaterial_frag_act_principal_os_list_view"
        app:layout_constraintStart_toStartOf="@+id/txtDescricaoBemMaterial_frag_act_principal_os_list_view"
        app:layout_constraintTop_toBottomOf="@+id/txtDescricaoBemMaterial_frag_act_principal_os_list_view" />

    <TextView
        android:id="@+id/txtStatusOS_frag_act_principal_os_list_view"
        android:layout_width="0dp"
        android:layout_height="14dp"
        android:layout_marginBottom="8dp"
        android:maxLines="1"
        android:text="Em Execução"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtPeriodoOS_content_act_bens_material_list_view"
        app:layout_constraintStart_toStartOf="@+id/txtTipoOcorrenciaBemMaterial_frag_act_principal_os_list_view"
        app:layout_constraintTop_toBottomOf="@+id/txtTipoOcorrenciaBemMaterial_frag_act_principal_os_list_view"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtPeriodoOS_content_act_bens_material_list_view"
        android:layout_width="184dp"
        android:layout_height="14dp"
        android:maxLines="1"
        android:text="01/02/2018 22:25 a 01/02/2018 22:25 "
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/txtTipoOcorrenciaBemMaterial_frag_act_principal_os_list_view"
        app:layout_constraintTop_toBottomOf="@+id/txtTipoOcorrenciaBemMaterial_frag_act_principal_os_list_view" />
</android.support.constraint.ConstraintLayout>