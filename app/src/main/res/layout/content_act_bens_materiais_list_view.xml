<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:layout_editor_absoluteY="25dp">

    <TextView
        android:id="@+id/txtDescricaoBemMaterial_content_act_bens_material_list_view"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:maxLines="1"
        android:text="Descrição Bem Material"
        android:textColor="@color/colorBlack"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtLocalizacaoBemMaterial_content_act_bens_material_list_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Localização"
        android:textSize="12dp"
        app:layout_constraintEnd_toEndOf="@+id/txtDescricaoBemMaterial_content_act_bens_material_list_view"
        app:layout_constraintStart_toStartOf="@+id/txtDescricaoBemMaterial_content_act_bens_material_list_view"
        app:layout_constraintTop_toBottomOf="@+id/txtDescricaoBemMaterial_content_act_bens_material_list_view" />

    <TextView
        android:id="@+id/txtStatusBemMaterial_content_act_bens_material_list_view"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:maxLines="1"
        android:text="Status"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/txtLocalizacaoBemMaterial_content_act_bens_material_list_view"
        app:layout_constraintStart_toStartOf="@+id/txtLocalizacaoBemMaterial_content_act_bens_material_list_view"
        app:layout_constraintTop_toBottomOf="@+id/txtLocalizacaoBemMaterial_content_act_bens_material_list_view"
        app:layout_constraintVertical_bias="1.0" />
</android.support.constraint.ConstraintLayout>